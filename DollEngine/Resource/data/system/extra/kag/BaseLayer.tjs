
class BaseLayer extends GraphicLayer
{
	function BaseLayer (app,lay) 
	{
		super.GraphicLayer(app,lay);
		setAnchor(0,1);
		setPos(0,lay.height,app.window.width,app.window.height);
	}

	function onSave () {
		var dict = super.onSave();
		with(dict){
			.className = "BaseLayer";
		}
		return dict;
	}
}