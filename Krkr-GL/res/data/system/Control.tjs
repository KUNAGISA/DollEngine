class Control extends KAGLayer
{
	var mainTouch;

	function Control (win,par) 
	{
		super.KAGLayer(win,par);
		mainTouch = addTouch();
		enabled = true;
	}

	function finalize () 
	{
		invalidate mainTouch;
		super.finalize();
	}

	function addTouch () 
	{
		var touch = new TouchEvent();
		touch.object = this;
		return touch;
	}

}